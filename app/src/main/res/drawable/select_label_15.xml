<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape>
            <gradient android:angle="270" android:endColor="@color/ic_coure_yiliu" android:startColor="@color/ic_coure_yiliu" />
            <size android:width="@dimen/rectangle_width" android:height="@dimen/rectangle_height" />
            <corners android:radius="@dimen/corners_radius" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <gradient android:angle="270" android:endColor="@color/ic_coure_yiliu" android:startColor="@color/ic_coure_yiliu" />
            <size android:width="@dimen/rectangle_width" android:height="@dimen/rectangle_height" />
            <corners android:radius="@dimen/corners_radius" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:angle="270" android:endColor="@color/ic_coure_yiliu_pr" android:startColor="@color/ic_coure_yiliu_pr" />
            <size android:width="@dimen/rectangle_width" android:height="@dimen/rectangle_height" />
            <corners android:radius="@dimen/corners_radius" />
        </shape>
    </item>
</selector>